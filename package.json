{
  "name": "play-platform-relay",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "engines": {
    "node": "12.18.1",
    "npm": "6.14.5"
  },
  "scripts": {
    "build": "better-npm-run build",
    "watch:build": "webpack -w",
    "watch:server-dev": "nodemon --verbose src/server/server.ts",
    "dev": "better-npm-run dev",
    "start": "node dist/server.js",
    "start:test": "better-npm-run test"
  },
  "betterScripts": {
    "dev": {
      "command": "npm-run-all --parallel watch:*",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8443,
        "TS_NODE_FILES": true
      }
    },
    "build": {
      "command": "webpack -w",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8443,
        "TS_NODE_FILES": true
      }
    },
    "test": {
      "command": "ts-node --files src/server/server.ts",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8443,
        "TS_NODE_FILES": true
      }
    }
  },
  "keywords": [
    "lightning",
    "lnd",
    "crypto",
    "bitcoin",
    "game",
    "sphinx",
    "n2n2"
  ],
  "author": "masterial",
  "license": "ISC",
  "dependencies": {
    "@bugsnag/js": "^7.9.2",
    "@bugsnag/plugin-express": "^7.9.2",
    "@types/express": "^4.17.11",
